  "terms": [
    {
      "term_text": "governance area",
      "normalized_term": "governance area",
      "term_id": "governance-area",
      "anchor": "#governance-area",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "bf500fe19d1f3ddf45ca7fdecacf9571d76fe0f1f03d5ac757b236fa089fb8a3",
      "definition_text_excerpt": "A legal and administrative domain that issues and governs identity items and their verification rules.",
      "imported_from": null
    },
    {
      "term_text": "physical identity item",
      "normalized_term": "physical identity item",
      "term_id": "physical-identity-item",
      "anchor": "#physical-identity-item",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "a52b33463b36a78e3864b5362a1e907281e4c6a03b4318e85219996dd826717c",
      "definition_text_excerpt": "A physical identity document or token issued under a governance area and recognized by that area for identity purposes.",
      "imported_from": null
    },
    {
      "term_text": "chip-backed private key",
      "normalized_term": "chipbacked private key",
      "term_id": "chip-backed-private-key",
      "anchor": "#chip-backed-private-key",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "0ee01a0747c0d9d5655c2ab8a8bfb843182b26a32f6e10cad7ce6b8dd951d3a9",
      "definition_text_excerpt": "Private-key material whose generation, storage, and signing operations are enforced by secure hardware boundaries.",
      "imported_from": null
    },
    {
      "term_text": "governance verification endpoint",
      "normalized_term": "governance verification endpoint",
      "term_id": "governance-verification-endpoint",
      "anchor": "#governance-verification-endpoint",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "38b4c3b7807e191b740096675f8e9650fd56cdb76e75ca79296c00fb2ae78708",
      "definition_text_excerpt": "A network endpoint exposing discovery and/or status information needed to validate document legitimacy within a governance area. In this profile, those endpoints are exposed under /.well-known/gidas/ .",
      "imported_from": null
    },
    {
      "term_text": "list",
      "normalized_term": "list",
      "term_id": "list",
      "anchor": "#list",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "e4168218475d7bb5150d637ef8229fb281b872460d9e6103d60c1f2a58437b23",
      "definition_text_excerpt": "Data concept used as defined by Infra [INFRA] .",
      "imported_from": null
    },
    {
      "term_text": "set",
      "normalized_term": "set",
      "term_id": "set",
      "anchor": "#set",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "e4168218475d7bb5150d637ef8229fb281b872460d9e6103d60c1f2a58437b23",
      "definition_text_excerpt": "Data concept used as defined by Infra [INFRA] .",
      "imported_from": null
    },
    {
      "term_text": "byte sequence",
      "normalized_term": "byte sequence",
      "term_id": "byte-sequence",
      "anchor": "#byte-sequence",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "e4168218475d7bb5150d637ef8229fb281b872460d9e6103d60c1f2a58437b23",
      "definition_text_excerpt": "Data concept used as defined by Infra [INFRA] .",
      "imported_from": null
    },
    {
      "term_text": "tuple",
      "normalized_term": "tuple",
      "term_id": "tuple",
      "anchor": "#tuple",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "e4168218475d7bb5150d637ef8229fb281b872460d9e6103d60c1f2a58437b23",
      "definition_text_excerpt": "Data concept used as defined by Infra [INFRA] .",
      "imported_from": null
    },
    {
      "term_text": "tuple expression",
      "normalized_term": "tuple expression",
      "term_id": "tuple-expression",
      "anchor": "#tuple-expression",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "e644cf4dd1a5f39fd14eeaf5f85dd5a47eb5e39fb5c402903a5a8fcbdf8ae176",
      "definition_text_excerpt": "Canonical serialized tuple string. In this specification, tuple expressions use : as the component delimiter, without leading/trailing whitespace around components.",
      "imported_from": null
    },
    {
      "term_text": "PID identification endpoint",
      "normalized_term": "pid identification endpoint",
      "term_id": "pid-identification-endpoint",
      "anchor": "#pid-identification-endpoint",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "f4b25794aa51110d3af94d33c2ca0a51defa8b136f3ee334df9924e68b83d9b5",
      "definition_text_excerpt": "A governance-recognized endpoint that publishes normative machine-readable PID requirements for a jurisdiction, including the required MRZ-derived fields and field-level validation patterns.",
      "imported_from": null
    },
    {
      "term_text": "PID field set",
      "normalized_term": "pid field set",
      "term_id": "pid-field-set",
      "anchor": "#pid-field-set",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "6fab225d701e02fbc76f16763148abe8778b354685c3a8dea4659dcb8dcc9977",
      "definition_text_excerpt": "A list of PID requirement entries for a governance area, serialized as a JSON array [RFC8259] . Membership semantics are a set keyed by fieldId (that is, fieldId values are unique). Each entry identifies a physical identity item field (as e",
      "imported_from": null
    },
    {
      "term_text": "MRZ",
      "normalized_term": "mrz",
      "term_id": "mrz",
      "anchor": "#mrz",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "468227f290703f863c7ba9883ea67b12645668ae9c8003cdc7ae7c60d76c9922",
      "definition_text_excerpt": "Machine Readable Zone data defined in ICAO Doc 9303 [ICAO-9303] [ICAO-9303-P3] .",
      "imported_from": null
    },
    {
      "term_text": "PID",
      "normalized_term": "pid",
      "term_id": "pid",
      "anchor": "#pid",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "0c5ad621deb1819b7ac24bd061ef895b8a6cc4c0e03368f0c7ab15cadeef6b87",
      "definition_text_excerpt": "A set of data that identifies a person under applicable law or standards contexts.",
      "imported_from": null
    },
    {
      "term_text": "PID hash",
      "normalized_term": "pid hash",
      "term_id": "pid-hash",
      "anchor": "#pid-hash",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "c773aacf5549bb879af86be2963d8d60f8cef0cb0f13f4d402daefbafcd56ba0",
      "definition_text_excerpt": "A cryptographic hash over canonicalized PID fields extracted from MRZ-compatible document data, represented as a hash tuple expression .",
      "imported_from": null
    },
    {
      "term_text": "GDIS binding credential",
      "normalized_term": "gdis binding credential",
      "term_id": "gdis-binding-credential",
      "anchor": "#gdis-binding-credential",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "55d2bb46703d2e5afe5443031bdaa46039f336c50c3f6691deb262ed9a205747",
      "definition_text_excerpt": "A verifiable credential binding a PID hash to a subject DID, issuer evidence, and governance verification material.",
      "imported_from": null
    },
    {
      "term_text": "GDIS subject DID",
      "normalized_term": "gdis subject did",
      "term_id": "gdis-subject-did",
      "anchor": "#gdis-subject-did",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "577a8872cef469d40d7c4b258bf9a37571ef5f53f3c47c1e85d55137ae85327b",
      "definition_text_excerpt": "Subject DID in a GDIS binding credential , controlled by a GQSCD-class controller.",
      "imported_from": null
    },
    {
      "term_text": "GQSCD controller",
      "normalized_term": "gqscd controller",
      "term_id": "gqscd-controller",
      "anchor": "#gqscd-controller",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "c0a742523bec04201e72283f81f74f788ba5c1b1040715f40d817c3ad86e0233",
      "definition_text_excerpt": "DID controller profile requiring protected key custody and signer-intent controls as defined by GQSCD Core [GQSCD-CORE] .",
      "imported_from": null
    },
    {
      "term_text": "decentralized event log",
      "normalized_term": "decentralized event log",
      "term_id": "decentralized-event-log",
      "anchor": "#decentralized-event-log",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "eba15d7541c4ceeee5b9404e4b280e8df0f525a785e967839db68a4647152414",
      "definition_text_excerpt": "Open-participation publication layer used to replicate GDIS verification material across many origins. Transport and host participation semantics are defined by the declared host profile (for example GQTS), not by this specification.",
      "imported_from": null
    },
    {
      "term_text": "host profile",
      "normalized_term": "host profile",
      "term_id": "host-profile",
      "anchor": "#host-profile",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "ba3347b2a836a0768e247f91253452ce493889ef1123a4d66f30810274b516d8",
      "definition_text_excerpt": "External specification that defines host role semantics, admission, transport, replication, and consistency behavior for publication participants (for example GQTS). [GQTS-CORE] .",
      "imported_from": null
    },
    {
      "term_text": "mandatory minimum hosts",
      "normalized_term": "mandatory minimum host",
      "term_id": "mandatory-minimum-hosts",
      "anchor": "#mandatory-minimum-hosts",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "40804cf7d20df63c02314af37bd730dfaa4c416c45f285569710a2d669e1fc85",
      "definition_text_excerpt": "Governance-required publication targets in the selected host profile that define a minimum publication floor and not an exclusive publication ceiling.",
      "imported_from": null
    },
    {
      "term_text": "publication set",
      "normalized_term": "publication set",
      "term_id": "publication-set",
      "anchor": "#publication-set",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "4af087998d2b49f83bf06cf85ab683ca6b888be8026dca5d7b04585cc43dba7d",
      "definition_text_excerpt": "Union of mandatory governance publication targets and person-selected additional targets to which subject materials are published.",
      "imported_from": null
    },
    {
      "term_text": "asOf query parameter",
      "normalized_term": "asof query parameter",
      "term_id": "asof-query-parameter",
      "anchor": "#asof-query-parameter",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "44d7ef64e437cae5dbf7cd4a96ff94459e611cd89d3fbd5f84ca264756da480d",
      "definition_text_excerpt": "URI query parameter whose value is an RFC 3339 date-time selecting the policy version that was effective at that instant [RFC3339] .",
      "imported_from": null
    },
    {
      "term_text": "evidence artifact",
      "normalized_term": "evidence artifact",
      "term_id": "evidence-artifact",
      "anchor": "#evidence-artifact",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "7317faf06554a2bb0cd3ca2904c8a486a5989942deeee30bbb796ea7498cacda",
      "definition_text_excerpt": "A signed or hashed machine-verifiable artifact used by issuers, host-profile operators, or verifiers to substantiate a security or status claim.",
      "imported_from": null
    },
    {
      "term_text": "conformance report",
      "normalized_term": "conformance report",
      "term_id": "conformance-report",
      "anchor": "#conformance-report",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "7996422db36b38a9e2c5d5c85755f75fa721f56c3883d5f7e2b61ea4ebb41117",
      "definition_text_excerpt": "Implementation report that states conformance class coverage, policy assumptions, test outputs, and produced or consumed evidence artifact types.",
      "imported_from": null
    },
    {
      "term_text": "verification result",
      "normalized_term": "verification result",
      "term_id": "verification-result",
      "anchor": "#verification-result",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "f12a1a5604e8e4a59c086babced35fdf95636f64ef8ea15d4596b74dd27104dd",
      "definition_text_excerpt": "Deterministic output object produced by a verifier after executing the normative processing rules in this specification.",
      "imported_from": null
    },
    {
      "term_text": "web profile",
      "normalized_term": "web profile",
      "term_id": "web-profile",
      "anchor": "#web-profile",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "3b6f2f4a858e9abfb5ed30087c236203e5f9d9309be1fb0f22a578725dfdd5b4",
      "definition_text_excerpt": "Imported term from [GQSCD-CORE] .",
      "imported_from": "GQSCD-CORE#web-profile"
    },
    {
      "term_text": "EU compatibility profile",
      "normalized_term": "eu compatibility profile",
      "term_id": "eu-compatibility-profile",
      "anchor": "#eu-compatibility-profile",
      "section_anchor": "#terminology",
      "definition_excerpt_hash": "3b6f2f4a858e9abfb5ed30087c236203e5f9d9309be1fb0f22a578725dfdd5b4",
